<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Default Suite">
    <test name="YandexMoney">
        <classes>
            <class name="org.panteleyev.money.ymoney.TestAuthResponse"/>
            <class name="org.panteleyev.money.ymoney.TestClientId"/>
        </classes>
    </test>
    <test name="DAO Classes">
        <classes>
            <class name="org.panteleyev.money.test.TestAccount"/>
            <class name="org.panteleyev.money.test.TestCategory"/>
            <class name="org.panteleyev.money.test.TestContact"/>
            <class name="org.panteleyev.money.test.TestCurrency"/>
            <class name="org.panteleyev.money.test.TestTransaction"/>
            <class name="org.panteleyev.money.test.TestTransactionGroup"/>
        </classes>
    </test>
    <test name="DAO">
        <classes>
            <class name="org.panteleyev.money.test.TestMoneyDAO"/>
            <class name="org.panteleyev.money.test.TestFreshFile"/>
        </classes>
    </test>
    <test name="Import/Export">
        <classes>
            <class name="org.panteleyev.money.test.TestImportExport"/>
            <class name="org.panteleyev.money.test.TestImportRecords"/>
            <class name="org.panteleyev.money.test.TestXsdNegative"/>
        </classes>
    </test>
    <test name="Profiles">
        <classes>
            <class name="org.panteleyev.money.test.profiles.TestConnectionProfile"/>
            <class name="org.panteleyev.money.test.profiles.TestConnectionProfileManager"/>
        </classes>
    </test>
    <test name="Misc">
        <classes>
            <class name="org.panteleyev.money.test.TestEditorPane"/>
            <class name="org.panteleyev.money.test.TestStatements"/>
        </classes>
    </test>
</suite>